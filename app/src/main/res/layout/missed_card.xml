<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="358dp"
    android:layout_height="145dp"
    android:layout_marginStart="2dp"
    android:layout_marginEnd="8dp"
    android:layout_margin="4dp">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >

        <TextView
            android:id="@+id/jeeMockTestMiC"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="18dp"
            android:fontFamily="@font/roboto_bold"
            android:text="JEE Mock Test 2021"
            android:textColor="#191919"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

        </TextView>

        <TextView
            android:id="@+id/mathsTextMiC"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="8dp"
            android:text="Mathematics"
            android:textColor="#636363"
            android:fontFamily="@font/roboto"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/jeeMockTestMiC">

        </TextView>

        <TextView
            android:id="@+id/dot1MiC"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:text="\u25CF"
            app:layout_constraintStart_toEndOf="@+id/mathsTextMiC"
            app:layout_constraintTop_toBottomOf="@+id/jeeMockTestMiC">

        </TextView>

        <TextView
            android:id="@+id/phyTextMiC"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:fontFamily="@font/roboto"
            android:text="Physics"
            android:textColor="#636363"
            app:layout_constraintStart_toEndOf="@+id/dot1MiC"
            app:layout_constraintTop_toBottomOf="@+id/jeeMockTestMiC">

        </TextView>

        <TextView
            android:id="@+id/dot2MiC"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:text="\u25CF"
            app:layout_constraintStart_toEndOf="@+id/phyTextMiC"
            app:layout_constraintTop_toBottomOf="@+id/jeeMockTestMiC">

        </TextView>

        <TextView
            android:id="@+id/chemTextMiC"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:fontFamily="@font/roboto"
            android:text="Chemistry"
            android:textColor="#636363"
            app:layout_constraintStart_toEndOf="@+id/dot2MiC"
            app:layout_constraintTop_toBottomOf="@+id/jeeMockTestMiC">

        </TextView>

        <TextView
            android:id="@+id/forfiveTextMiC"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="8dp"
            android:text="45Q"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/mathsTextMiC">

        </TextView>

        <TextView
            android:id="@+id/dot3MiC"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:text="\u25CF"
            app:layout_constraintStart_toEndOf="@+id/forfiveTextMiC"
            app:layout_constraintTop_toBottomOf="@+id/mathsTextMiC">

        </TextView>

        <TextView
            android:id="@+id/sevminTextMiC"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:text="60 mins"
            app:layout_constraintStart_toEndOf="@+id/dot3MiC"
            app:layout_constraintTop_toBottomOf="@+id/mathsTextMiC">

        </TextView>

        <TextView
            android:id="@+id/dot4MiC"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:text="\u25CF"
            app:layout_constraintStart_toEndOf="@+id/sevminTextMiC"
            app:layout_constraintTop_toBottomOf="@+id/mathsTextMiC">

        </TextView>

        <TextView
            android:id="@+id/marksTextMiC"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:text="75 marks"
            app:layout_constraintStart_toEndOf="@+id/dot4MiC"
            app:layout_constraintTop_toBottomOf="@+id/mathsTextMiC">

        </TextView>

        <TextView
            android:id="@+id/recentPublishTextMiC"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="20dp"
            android:fontFamily="@font/roboto"
            android:text="Test Expired"
            android:textColor="#636363"
            android:textSize="14dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/sevminTextMiC">

        </TextView>

        <TextView
            android:id="@+id/viewresultextMiC"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="128dp"
            android:layout_marginTop="20dp"
            android:fontFamily="@font/roboto_bold"
            android:text="View Solutions"
            android:textColor="#1089FF"
            android:textSize="14dp"
            app:layout_constraintStart_toEndOf="@+id/recentPublishTextMiC"
            app:layout_constraintTop_toBottomOf="@+id/dot4MiC">

        </TextView>

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="22dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="12dp"
            android:src="@drawable/keybackspcp1"
            app:layout_constraintStart_toEndOf="@+id/viewresultextMiC"
            app:layout_constraintTop_toBottomOf="@+id/dot4MiC">

        </ImageView>

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.cardview.widget.CardView>