<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/cardviewSheet"
    app:cardCornerRadius="12dp">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <ImageView
            android:layout_width="28dp"
            android:layout_height="30dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="24dp"
            android:background="@color/white"
            android:src="@drawable/close1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent">

        </ImageView>

        <TextView
            android:id="@+id/textView109"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="56dp"
            android:fontFamily="@font/roboto_bold"
            android:text="TIME UP!"
            android:textColor="#191919"
            android:textSize="18dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

        </TextView>

        <TextView
            android:id="@+id/textView110"
            android:layout_width="302dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:fontFamily="@font/roboto"
            android:text="Your time is up. Your answers have been submitted successfully."
            android:textColor="#636363"
            android:textSize="14dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.496"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView109">

        </TextView>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout"
            android:layout_width="302dp"
            android:layout_height="282dp"
            android:layout_marginTop="32dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView110">

            <ImageView
                android:id="@+id/stopwatch"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="32dp"
                android:layout_marginTop="28dp"
                android:src="@drawable/stopwatch"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

            </ImageView>

            <TextView
                android:id="@+id/timeText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="28dp"
                android:fontFamily="@font/roboto_bold"
                android:text="00 : 00 : 05"
                android:textColor="#191919"
                android:textSize="14dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.588"
                app:layout_constraintStart_toEndOf="@+id/timeLeft"
                app:layout_constraintTop_toTopOf="parent">

            </TextView>

            <TextView
                android:id="@+id/timeLeft"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="28dp"
                android:fontFamily="@font/roboto"
                android:text="Time left"
                android:textColor="#191919"
                android:textSize="14dp"
                app:layout_constraintStart_toEndOf="@+id/stopwatch"
                app:layout_constraintTop_toTopOf="parent">

            </TextView>

            <ImageView
                android:id="@+id/attemptImgp39"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="32dp"
                android:layout_marginTop="28dp"
                android:src="@drawable/attempted"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/stopwatch">

            </ImageView>

            <TextView
                android:id="@+id/attemptedCountp39"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="28dp"
                android:fontFamily="@font/roboto_bold"
                android:text="24"
                android:textColor="#191919"
                android:textSize="14dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.7"
                app:layout_constraintStart_toEndOf="@+id/attemptedTextp39"
                app:layout_constraintTop_toBottomOf="@id/stopwatch">

            </TextView>

            <TextView
                android:id="@+id/attemptedTextp39"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="28dp"
                android:fontFamily="@font/roboto"
                android:text="Attempted"
                android:textColor="#191919"
                android:textSize="14dp"
                app:layout_constraintStart_toEndOf="@+id/attemptImgp39"
                app:layout_constraintTop_toBottomOf="@id/stopwatch">

            </TextView>

            <ImageView
                android:id="@+id/unattemptImgp39"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="32dp"
                android:layout_marginTop="28dp"
                android:src="@drawable/unattempted"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/attemptImgp39">

            </ImageView>

            <TextView
                android:id="@+id/unattemptedCountp39"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="28dp"
                android:fontFamily="@font/roboto_bold"
                android:text="12"
                android:textColor="#191919"
                android:textSize="14dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.7"
                app:layout_constraintStart_toEndOf="@+id/attemptedTextp39"
                app:layout_constraintTop_toBottomOf="@id/attemptImgp39">

            </TextView>

            <TextView
                android:id="@+id/unattemptedTextp39"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="28dp"
                android:fontFamily="@font/roboto"
                android:text="UnAttempted"
                android:textColor="#191919"
                android:textSize="14dp"
                app:layout_constraintStart_toEndOf="@+id/unattemptImgp39"
                app:layout_constraintTop_toBottomOf="@id/attemptImgp39">

            </TextView>

            <ImageView
                android:id="@+id/attemptMrImgp39"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="32dp"
                android:layout_marginTop="28dp"
                android:src="@drawable/start_ic"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/unattemptImgp39">

            </ImageView>

            <TextView
                android:id="@+id/attemptedMrCountp39"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="28dp"
                android:fontFamily="@font/roboto_bold"
                android:text="8"
                android:textColor="#191919"
                android:textSize="14dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.715"
                app:layout_constraintStart_toEndOf="@+id/attemptedTextp39"
                app:layout_constraintTop_toBottomOf="@id/unattemptImgp39">

            </TextView>

            <TextView
                android:id="@+id/attemptedMrTextp39"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="28dp"
                android:fontFamily="@font/roboto"
                android:text="Attemted and Marked"
                android:textColor="#191919"
                android:textSize="14dp"
                app:layout_constraintStart_toEndOf="@+id/attemptMrImgp39"
                app:layout_constraintTop_toBottomOf="@id/unattemptImgp39">

            </TextView>

            <ImageView
                android:id="@+id/unattemptMrImgp39"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="32dp"
                android:layout_marginTop="28dp"
                android:src="@drawable/start_ic"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/attemptMrImgp39">

            </ImageView>

            <TextView
                android:id="@+id/unattemptedMrCountp39"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="24dp"
                android:fontFamily="@font/roboto_bold"
                android:text="8"
                android:textColor="#191919"
                android:textSize="14dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.715"
                app:layout_constraintStart_toEndOf="@+id/attemptedTextp39"
                app:layout_constraintTop_toBottomOf="@id/attemptMrImgp39">

            </TextView>

            <TextView
                android:id="@+id/unattemptedMrTextp39"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="28dp"
                android:fontFamily="@font/roboto"
                android:text="Unattempted and marked "
                android:textColor="#191919"
                android:textSize="14dp"
                app:layout_constraintStart_toEndOf="@+id/unattemptMrImgp39"
                app:layout_constraintTop_toBottomOf="@id/attemptMrImgp39">

            </TextView>

            <ImageView
                android:id="@+id/unseenImgp39"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="32dp"
                android:layout_marginTop="28dp"
                android:src="@drawable/not_disturb"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/unattemptMrImgp39">

            </ImageView>

            <TextView
                android:id="@+id/unseenCountp39"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="28dp"
                android:fontFamily="@font/roboto_bold"
                android:text="3"
                android:textColor="#191919"
                android:textSize="14dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.715"
                app:layout_constraintStart_toEndOf="@+id/attemptedTextp39"
                app:layout_constraintTop_toBottomOf="@id/unattemptMrImgp39">

            </TextView>

            <TextView
                android:id="@+id/unseenTextp39"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="12dp"
                android:layout_marginTop="28dp"
                android:fontFamily="@font/roboto"
                android:text="Unseen"
                android:textColor="#191919"
                android:textSize="14dp"
                app:layout_constraintStart_toEndOf="@+id/unseenImgp39"
                app:layout_constraintTop_toBottomOf="@id/unattemptMrImgp39">

            </TextView>

        </androidx.constraintlayout.widget.ConstraintLayout>


        <TextView
            android:id="@+id/textView115"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:fontFamily="@font/roboto"
            android:text="Results will be published on or before"
            android:textColor="#636363"
            android:textSize="12dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.502"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/constraintLayout">

        </TextView>

        <TextView
            android:id="@+id/textView114"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:fontFamily="@font/roboto_bold"
            android:text="12th July,2021"
            android:textColor="#191919"
            android:textSize="18dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView115">

        </TextView>

        <Button
            android:layout_width="302dp"
            android:layout_height="48dp"
            android:layout_marginBottom="12dp"
            android:background="@drawable/custom_button"
            android:text="SUBMIT"
            android:textColor="#FFFFFF"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.504"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView114"
            app:layout_constraintVertical_bias="0.121">

        </Button>

    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.cardview.widget.CardView>