<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="358dp"
    android:layout_height="145dp"
    android:layout_marginStart="2dp"
    android:layout_marginEnd="8dp"
    android:layout_margin="4dp">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >

        <TextView
            android:id="@+id/jeeMockTestSC1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="18dp"
            android:fontFamily="@font/roboto_bold"
            android:text="JEE Mock Test 2021"
            android:textColor="#191919"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

        </TextView>

        <TextView
            android:id="@+id/mathsTextSC1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="8dp"
            android:text="Mathematics"
            android:textColor="#636363"
            android:fontFamily="@font/roboto"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/jeeMockTestSC1">

        </TextView>

        <TextView
            android:id="@+id/dot1SC1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:text="\u25CF"
            app:layout_constraintStart_toEndOf="@+id/mathsTextSC1"
            app:layout_constraintTop_toBottomOf="@+id/jeeMockTestSC1">

        </TextView>

        <TextView
            android:id="@+id/phyTextSC1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:fontFamily="@font/roboto"
            android:text="Physics"
            android:textColor="#636363"
            app:layout_constraintStart_toEndOf="@+id/dot1SC1"
            app:layout_constraintTop_toBottomOf="@+id/jeeMockTestSC1">

        </TextView>

        <TextView
            android:id="@+id/dot2SC1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:text="\u25CF"
            app:layout_constraintStart_toEndOf="@+id/phyTextSC1"
            app:layout_constraintTop_toBottomOf="@+id/jeeMockTestSC1">

        </TextView>

        <TextView
            android:id="@+id/chemTextSC1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:fontFamily="@font/roboto"
            android:text="Chemistry"
            android:textColor="#636363"
            app:layout_constraintStart_toEndOf="@+id/dot2SC1"
            app:layout_constraintTop_toBottomOf="@+id/jeeMockTestSC1">

        </TextView>

        <TextView
            android:id="@+id/forfiveTextSC1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="8dp"
            android:text="45Q"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/mathsTextSC1">

        </TextView>

        <TextView
            android:id="@+id/dot3SC1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:text="\u25CF"
            app:layout_constraintStart_toEndOf="@+id/forfiveTextSC1"
            app:layout_constraintTop_toBottomOf="@+id/mathsTextSC1">

        </TextView>

        <TextView
            android:id="@+id/sevminTextSC1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:text="60 mins"
            app:layout_constraintStart_toEndOf="@+id/dot3SC1"
            app:layout_constraintTop_toBottomOf="@+id/mathsTextSC1">

        </TextView>

        <TextView
            android:id="@+id/dot4SC1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:text="\u25CF"
            app:layout_constraintStart_toEndOf="@+id/sevminTextSC1"
            app:layout_constraintTop_toBottomOf="@+id/mathsTextSC1">

        </TextView>

        <TextView
            android:id="@+id/marksTextSC1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:text="75 marks"
            app:layout_constraintStart_toEndOf="@+id/dot4SC1"
            app:layout_constraintTop_toBottomOf="@+id/mathsTextSC1">

        </TextView>

        <TextView
            android:id="@+id/recentPublishTextSC1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="20dp"
            android:fontFamily="@font/roboto"
            android:text="Result published on 12th July,2021"
            android:textColor="#636363"
            android:textSize="14dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/sevminTextSC1">

        </TextView>

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.cardview.widget.CardView>